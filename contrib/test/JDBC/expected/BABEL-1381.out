CREATE PROCEDURE p_babel_1381_tinyint (@a tinyint OUTPUT) AS
BEGIN
  SET @a=42;
  select @a as a;
END;
GO

EXEC p_babel_1381_tinyint 1;
GO
~~START~~
tinyint
42
~~END~~


